package com.ReneeThomas.builtInClassPractice;
//import any built in classes
import java.util.Hashtable;
import java.io.FileReader;
import java.io.FileWriter;

public class builtInClassPractice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Hashtable<String, String> englishDictionary = new Hashtable <String, String>();
		
		// add an entry into the englishDictionary
		englishDictionary.put("Insanity", "Doing the same thing over and over again expecting a different result.");
		
		String definition = englishDictionary.get("Insanity");
		
		System.out.println("Insanity = " + definition);
	}

}
