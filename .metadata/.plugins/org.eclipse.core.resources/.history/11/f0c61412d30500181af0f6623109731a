package com.ReneeThomas.objectClassPractice;

public class student {

	//class variables
	String firstName;
	String lastName;
	String courseFocus;
	
	//create constructors
	public student() {
		firstName="";
		lastName="";
		courseFocus="";
		System.out.println("Student object created!");
	}
	
	//overloaded constructor
	public student(String fName, String lName, String focus) {
		firstName = fName;
		lastName = lName;
		courseFocus = focus;
		System.out.println(fName + " " + lName + " Has signed up for " + focus + ".");
	}
	
	//getter methods
	public string getFirstName() {
		return(firstName);
	}
	
}






